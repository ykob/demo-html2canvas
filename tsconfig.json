{
  "include": [
    "src",
    "styled-system"
  ],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "styled-system": [
        "./styled-system/*"
      ]
    },
    "types": [
      "vite/client"
    ]
  }
}